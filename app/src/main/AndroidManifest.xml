<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tranquil">
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".iq"
            android:parentActivityName=".management"></activity>
        <activity
            android:name=".detect"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Music"
            android:parentActivityName=".management" />
        <activity
            android:name=".Exercise"
            android:parentActivityName=".management" />
        <activity
            android:name=".Yoga"
            android:parentActivityName=".management" />
        <activity
            android:name=".Mantra"
            android:parentActivityName=".management" />
        <activity
            android:name=".Meditate"
            android:parentActivityName=".management" />
        <activity
            android:name=".Breathe"
            android:parentActivityName=".management" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Analysis"
            android:label="Analysis"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".management"
            android:label="Management"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".AboutStress"
            android:label="About Stress"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".calendar"
            android:label="Calendar"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Reminder"
            android:label="Reminder"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Detection"
            android:label="Detection"
            android:parentActivityName=".MainActivity" />
    </application>

</manifest>